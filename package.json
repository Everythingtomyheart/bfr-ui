{
  "name": "bfr-ui",
  "version": "0.0.1",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "scripts": {
    "bootstrap": "yarn --frozen-lockfile && npx lerna bootstrap",
    "link": "cd packages && cd bfr-ui && yarn link",
    "build": "yarn bootstrap && yarn clean:lib && yarn build:esm-bundle && yarn build:lib && yarn build:lib-full && yarn build:esm && yarn build:theme",
    "clean:lib": "rimraf lib && rimraf es",
    "build:lib": "cross-env LIBMODE=core webpack --config ./build/webpack.config.js",
    "build:lib-full": "cross-env LIBMODE=full webpack --config ./build/webpack.config.js",
    "build:esm-bundle": "rollup --config build/rollup.config.esm.bundle.js && yarn build:type",
    "build:type": "node build/gen-type.js",
    "build:esm": "node build/rollup.config.esm.js",
    "build:theme": "gulp build --gulpfile build/gulpfile.js",
    "build:theme1": "rollup --config build/rollup.less.js",
    "website-dev": "webpack-dev-server --config ./website/webpack.config.js",
    "website-dev:play": "cross-env PLAY_ENV=true yarn website-dev",
    "website-build": "rimraf website-dist && cross-env NODE_ENV=production webpack --config ./website/webpack.config.js"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@types/lodash": "^4.14.161",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@vue/babel-plugin-jsx": "^1.0.0-rc.3",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/test-utils": "^1.1.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "eslint": "^7.7.0",
    "eslint-plugin-vue": "^7.2.0",
    "file-loader": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "highlight.js": "^10.1.2",
    "html-webpack-plugin": "^4.3.0",
    "lerna": "^3.22.1",
    "less": "3.13.0",
    "less-loader": "^7.2.1",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "mini-css-extract-plugin": "^0.11.2",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^2.35.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "throttle-debounce": "2.3.0",
    "transliteration": "^2.2.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.0",
    "vue-loader": "^16.1.1",
    "vue-router": "^4.0.1",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "ant-design-vue": "^2.0.0-rc.7",
    "dayjs": "1.x",
    "lodash": "^4.17.20",
    "mitt": "^2.1.0",
    "sortablejs": "^1.10.2",
    "vue": "3.0.4"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.esm.js",
  "unpkg": "lib/index.js",
  "style": "lib/theme/index.css",
  "files": [
    "lib",
    "packages"
  ]
}
